<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Wordle">

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Enter word"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="50dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Submit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="598dp" />

    <TextView
        android:id="@+id/tb_1"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:layout_marginTop="60dp"
        android:rotationX="7"
        android:text="TextView"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName"
        tools:layout_editor_absoluteX="155dp" />

    <TextView
        android:id="@+id/tb_2"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:rotationX="7"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/tb_6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tb_1"
        app:layout_constraintVertical_bias=".0758" />

    <TextView
        android:id="@+id/tb_3"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:rotationX="7"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/tb_6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tb_1"
        app:layout_constraintVertical_bias=".3586" />

    <TextView
        android:id="@+id/tb_4"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:layout_marginTop="8dp"
        android:rotationX="7"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/tb_6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tb_1"
        app:layout_constraintVertical_bias=".6414" />

    <TextView
        android:id="@+id/tb_5"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:rotationX="7"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/tb_6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tb_1"
        app:layout_constraintVertical_bias=".9242" />

    <TextView
        android:id="@+id/tb_6"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:layout_marginBottom="140dp"
        android:rotationX="7"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>